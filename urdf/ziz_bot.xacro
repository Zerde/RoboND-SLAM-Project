<?xml version='1.0'?>
<robot name="ziz_bot" xmlns:xacro="http://www.ros.org/wiki/xacro">
    <xacro:include filename="$(find slam_project)/urdf/ziz_bot.gazebo">
    </xacro:include>
    <material name="blue">
        <color rgba="0 0 0.6 1">
        </color>
    </material>
    <material name="white">
        <color rgba="1 1 1 1">
        </color>
    </material>
    <material name="grey">
        <color rgba="192 192 192 1">
        </color>
    </material>
    <material name="black">
        <color rgba="0 0 0 1">
        </color>
    </material>
    <link name="robot_footprint"/>
    <joint name="robot_footprint_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0 0 0">
        </origin>
        <parent link="robot_footprint">
        </parent>
        <child link="base_cylinder">
        </child>
    </joint>
    <link name="base_cylinder">
        <pose>
            0 0 0.1 0 0 0
        </pose>
        <inertial>
            <mass value="15.0">
            </mass>
            <origin rpy=" 0 0 0" xyz="0.0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy=" 0 0 0" xyz="0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.1" radius="0.2">
                </cylinder>
            </geometry>
        </collision>
        <visual name="base_cylinder_visual">
            <geometry>
                <cylinder length="0.1" radius="0.2">
                </cylinder>
            </geometry>
            <material name="white">
            </material>
        </visual>
    </link> 
    <!--base_box -->
    <link name="base_box">
        <inertial>
            <mass value="5.0">
            </mass>
            <origin rpy=" 0 0 0" xyz="0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy=" 0 0 0" xyz="0 0 0">
            </origin>
            <geometry>
                <box size="0.15 0.5 0.04">
                </box>
            </geometry>
        </collision>
        <visual>
            <geometry>
                <box size="0.15 0.5 0.04">
                </box>
            </geometry>
            <material name="blue">
            </material>
        </visual>
    </link>
    <joint name="base_box_joint" type="fixed">
        <parent link="base_cylinder">
        </parent>
        <child link="base_box">
        </child>
        <origin xyz="0 0 0">
        </origin>
    </joint>
    <!--left_wheel-->
    <link name="left_wheel">
        <inertial>
            <mass value="5.0">
            </mass>
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.1">
                </cylinder>
            </geometry>
        </collision>
        <visual name="left_wheel_visual">
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.1">
                </cylinder>
            </geometry>
            <material name="black">
            </material>
        </visual>
    </link>
    <joint name="left_wheel_hinge" type="continuous">
        <origin rpy="0 0 0" xyz="0 0.27 0">
        </origin>
        <child link="left_wheel">
        </child>
        <parent link="base_box">
        </parent>
        <axis rpy="0 0 0" xyz="0 1 0">
        </axis>
        <limit effort="10000" velocity="1000">
        </limit>
        <joint_properties damping="1.0" friction="1.0">
        </joint_properties>
    </joint>
    <!--right_wheel-->
    <link name="right_wheel">
        <inertial>
            <mass value="5.0">
            </mass>
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.1">
                </cylinder>
            </geometry>
        </collision>
        <visual name="right_wheel_visual">
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.1">
                </cylinder>
            </geometry>
            <material name="black">
            </material>
        </visual>
    </link>
    <joint name="right_wheel_hinge" type="continuous">
        <origin rpy="0 0 0" xyz="0 -0.27 0">
        </origin>
        <child link="right_wheel">
        </child>
        <parent link="base_box">
        </parent>
        <axis rpy="0 0 0" xyz="0 1 0">
        </axis>
        <limit effort="10000" velocity="1000">
        </limit>
        <joint_properties damping="1.0" friction="1.0">
        </joint_properties>
    </joint>
    <!--back wheel-->
    <link name="back_wheel">
        <inertial>
            <mass value="3.0">
            </mass>
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision_back_wheel">
            <origin rpy=" 0 1.5707 1.5707" xyz="0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.0499">
                </cylinder>
            </geometry>
        </collision>
        <visual>
            <origin rpy=" 0 1.5707 1.5707" xyz="0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.05">
                </cylinder>
            </geometry>
            <material name="grey">
            </material>
        </visual>
    </link>
    <joint name="back_wheel_hinge" type="continuous">
        <origin rpy="0 0 0" xyz="-0.13 0 -0.05">
        </origin>
        <child link="back_wheel">
        </child>
        <parent link="base_cylinder">
        </parent>
        <axis rpy="0 0 0" xyz="0 1 0">
        </axis>
        <limit effort="10000" velocity="1000">
        </limit>
        <joint_properties damping="1.0" friction="2.0">
        </joint_properties>
    </joint>
   <!--front wheel-->
    <link name="front_wheel">
        <inertial>
            <mass value="3.0">
            </mass>
            <origin rpy=" 0 1.5707 1.5707" xyz="0.0 0 0">
            </origin>
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.1" iyz="0" izz="0.1">
            </inertia>
        </inertial>
        <collision name="collision_front_wheel">
            <origin rpy=" 0 1.5707 1.5707" xyz="0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.0499">
                </cylinder>
            </geometry>
        </collision>
        <visual>
            <origin rpy=" 0 1.5707 1.5707" xyz="0 0 0">
            </origin>
            <geometry>
                <cylinder length="0.05" radius="0.05">
                </cylinder>
            </geometry>
            <material name="grey">
            </material>
        </visual>
    </link>
    <joint name="front_wheel_hinge" type="continuous">
        <origin rpy="0 0 0" xyz="0.13 0 -0.05">
        </origin>
        <child link="front_wheel">
        </child>
        <parent link="base_cylinder">
        </parent>
        <axis rpy="0 0 0" xyz="0 1 0">
        </axis>
        <limit effort="10000" velocity="1000">
        </limit>
        <joint_properties damping="1.0" friction="2.0">
        </joint_properties>
    </joint>
    <!--hokuyo-->
    <link name="hokuyo">
        <inertial>
            <mass value="0.1">
            </mass>
            <origin rpy="0 0 0" xyz="0.0 0 0">
            </origin>
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy="0 0 0" xyz="0.0 0 0">
            </origin>
            <geometry>
                <box size="0.1 0.1 0.1">
                </box>
            </geometry>
        </collision>
        <visual name="hokuyo_visual">
            <origin rpy=" 0 0 0" xyz="0.0 0 0">
            </origin>
            <geometry>
                <mesh filename="package://slam_project/meshes/hokuyo.dae">
                </mesh>
            </geometry>
            <material name="red">
            </material>
        </visual>
    </link>
    <joint name="hokuyo_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.13 0 0.09">
        </origin>
        <child link="hokuyo">
        </child>
        <parent link="base_cylinder">
        </parent>
    </joint>
    <!--camera-->
    <link name="camera">
        <inertial>
            <mass value="0.1">
            </mass>
            <origin rpy="0 0 0" xyz="0.0 0 0">
            </origin>
            <inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6">
            </inertia>
        </inertial>
        <collision name="collision">
            <origin rpy="0 0 0" xyz="0.0 0 0">
            </origin>
            <geometry>
                <box size="0.05 0.05 0.05">
                </box>
            </geometry>
        </collision>
        <visual name="camera_visual">
            <origin rpy=" 0 0 0" xyz="0.0 0 0">
            </origin>
            <geometry>
                <box size="0.05 0.05 0.05">
                </box>
            </geometry>
            <material name="blue">
            </material>
        </visual>
    </link>
    <joint name="camera_joint" type="fixed">
        <origin rpy="0 0 0" xyz="0.2 0 0">
        </origin>
        <child link="camera">
        </child>
        <parent link="base_cylinder">
        </parent>
    </joint>
</robot>
